# Configurações do Supabase
# Obtenha essas informações no painel do Supabase (https://supabase.com/dashboard)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Configurações da aplicação
VITE_APP_TITLE=AI Squads Academy
VITE_APP_DESCRIPTION=Plataforma de ensino com IA

# Configurações de desenvolvimento
VITE_DEV_MODE=true
VITE_API_BASE_URL=http://localhost:8080

# Configurações de autenticação
VITE_AUTH_REDIRECT_URL=http://localhost:8080/auth/callback

# Configurações de upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Configurações de IA (OpenAI)
# Obtenha sua chave em: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your_openai_api_key_here

# Configurações de IA (Corcel)
# Obtenha sua chave em: https://corcel.io/dashboard
CORCEL_API_KEY=your_corcel_api_key_here

# Configurações de pagamento (Stripe)
# Obtenha suas chaves em: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret_here

# Configurações de email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password_here

# Configurações de logs
VITE_LOG_LEVEL=info

# Configurações de cache
VITE_CACHE_TTL=3600

# URL base para produção
VITE_PRODUCTION_URL=https://your-domain.com